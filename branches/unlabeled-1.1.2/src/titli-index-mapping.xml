<?xml version="1.0" encoding="UTF-8"?>

<titli>
	
	<tablejoinmapping>
		<table name="catissue_specimen" alias="s" keyColumn="IDENTIFIER" selectColumns="LABEL,BARCODE,AVAILABLE_QUANTITY,COLLECTION_STATUS">
			<join tableName="catissue_abstract_specimen" alias="a" keyColumn="IDENTIFIER" joinColumn="IDENTIFIER" selectColumns="SPECIMEN_CLASS,SPECIMEN_TYPE,LINEAGE,PATHOLOGICAL_STATUS,INITIAL_QUANTITY">
					<leftjoin tableName="catissue_specimen_char" alias="sc" keyColumn="IDENTIFIER" joinColumn="SPECIMEN_CHARACTERISTICS_ID" selectColumns="TISSUE_SITE,TISSUE_SIDE">
					</leftjoin>	
			</join>		
		</table>
		<table name="catissue_specimen_coll_group" alias="scg" keyColumn="IDENTIFIER" selectColumns="NAME,BARCODE,COLLECTION_STATUS">
			<join tableName="catissue_abs_speci_coll_group" alias="ascg" keyColumn="IDENTIFIER" joinColumn="IDENTIFIER" selectColumns="CLINICAL_DIAGNOSIS,CLINICAL_STATUS">
			</join>
		</table>
		<table name="catissue_storage_type" alias="sat" keyColumn="IDENTIFIER" selectColumns="DEFAULT_TEMPERATURE">
			<join tableName="catissue_container_type" alias="cat" keyColumn="IDENTIFIER" joinColumn="IDENTIFIER" activityStatus="ACTIVITY_STATUS" selectColumns="NAME,ONE_DIMENSION_LABEL,TWO_DIMENSION_LABEL">
			</join>
		</table>
		<table name="catissue_specimen_array_type" alias="spat" keyColumn="IDENTIFIER" includeColumns="SPECIMEN_CLASS">
			<join tableName="catissue_container_type" alias="ct" keyColumn="IDENTIFIER" joinColumn="IDENTIFIER" activityStatus="ACTIVITY_STATUS" selectColumns="NAME,ONE_DIMENSION_LABEL,TWO_DIMENSION_LABEL">
			</join>
		</table>
		<table name="catissue_user" alias="u" keyColumn="IDENTIFIER" includeColumns="FIRST_NAME,LAST_NAME,EMAIL_ADDRESS,ACTIVITY_STATUS" activityStatus="ACTIVITY_STATUS">
			<leftJoin tableName="catissue_address" alias="addr" keyColumn="IDENTIFIER" joinColumn="ADDRESS_ID" selectColumns="STREET,CITY,STATE,COUNTRY,ZIPCODE,PHONE_NUMBER">
			</leftJoin>
		</table>
		<table name="catissue_participant" alias="p" keyColumn="IDENTIFIER" selectColumns="GENOTYPE,ETHNICITY,GENDER,VITAL_STATUS" activityStatus="ACTIVITY_STATUS">
			<leftJoin tableName="catissue_race" alias="rc" keyColumn="PARTICIPANT_ID" joinColumn="IDENTIFIER" selectColumns="RACE_NAME">
			</leftJoin>
		</table>
		<table name="catissue_distribution_protocol" alias="dp" keyColumn="IDENTIFIER">
			<join tableName="catissue_specimen_protocol" alias="sp" keyColumn="IDENTIFIER" joinColumn="IDENTIFIER" selectColumns="TITLE,SHORT_TITLE" activityStatus="ACTIVITY_STATUS">
			</join>
		</table>
		<table name="catissue_collection_protocol" alias="cop" keyColumn="IDENTIFIER">
			<join tableName="catissue_specimen_protocol" alias="sep" keyColumn="IDENTIFIER" joinColumn="IDENTIFIER" selectColumns="TITLE,SHORT_TITLE" activityStatus="ACTIVITY_STATUS">
			</join>
		</table>
		<table name="catissue_specimen_array" alias="sa" keyColumn="IDENTIFIER">
			<join tableName="catissue_container" alias="c" keyColumn="IDENTIFIER" joinColumn="IDENTIFIER" selectColumns="NAME,BARCODE" activityStatus="ACTIVITY_STATUS">
				<leftjoin tableName="catissue_capacity" alias="cap" keyColumn = "IDENTIFIER" joinColumn="CAPACITY_ID" selectColumns="ONE_DIMENSION_CAPACITY,TWO_DIMENSION_CAPACITY">
					</leftjoin>	
			</join>
		</table>
		<table name="catissue_storage_container" alias="stc" keyColumn="IDENTIFIER" selectColumns="TEMPERATURE">
			<join tableName="catissue_container" alias="cn" keyColumn="IDENTIFIER" joinColumn="IDENTIFIER" selectColumns="NAME,BARCODE" activityStatus="ACTIVITY_STATUS">
				<leftjoin tableName="catissue_capacity" alias="cpc" keyColumn = "IDENTIFIER" joinColumn="CAPACITY_ID" selectColumns="ONE_DIMENSION_CAPACITY,TWO_DIMENSION_CAPACITY">
					</leftjoin>	
			</join>
		</table>
	</tablejoinmapping>
</titli>